<!DOCTYPE html>
<html>
<head>
  <title>Welcome Friends</title>
  <%= stylesheet_link_tag  'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css' %> 
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

  <%= javascript_include_tag  'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js' %>
    
    <%= stylesheet_link_tag   'application', media: 'all' %>    
  <%= csrf_meta_tags %>
</head>
<body>
    <div class='row'>
      <div class='col-md-12'>
        <%= render  'welcome/header' %> 
      </div>
    </div>
    <div class'row'>
      <div class='col-md-12'>
        <%= yield %>
      </div>
    </div>
    <div class'row'>
      <div class='col-md-12'>
        <%= render  'welcome/footer' %> 
      </div>
    </div> 
</body>
</html>
