<div class='container'>
<p id="notice"><%= notice %></p>

<%= link_to 'New User admin', new_user_admin_path, class: 'btn btn-primary' %>
<h1>Listing Admins </h1>
<table class='table table-condensed table-bordered'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Username</th>
      <th>Password</th>
    </tr>
  </thead>

  <tbody>
    <% @user_admins.each do |user_admin| %>
      <tr>
        <td><%= user_admin.name %></td>
        <td><%= user_admin.username %></td>
        <td><%= user_admin.password %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
